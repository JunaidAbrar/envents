{
  "name": "theme",
  "version": "4.0.1",
  "description": "",
  "scripts": {
    "start": "npm run dev",
    "build": "npm run build:clean && npm run build:tailwind && npm run copy:static",
    "build:clean": "rimraf ../static/css/dist && rimraf ../../static/css/dist",
    "build:tailwind": "cross-env NODE_ENV=production postcss ./src/master-styles.css -o ../static/css/dist/styles.css --minify",
    "copy:static": "mkdir -p ../../static/css/dist && cp -r ../static/css/dist/styles.css ../../static/css/dist/",
    "dev": "cross-env NODE_ENV=development postcss ./src/master-styles.css -o ../static/css/dist/styles.css --watch",
    "build:prod": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.7",
    "postcss": "^8.4.24",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-simple-vars": "^7.0.1",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.3.2"
  }
}
